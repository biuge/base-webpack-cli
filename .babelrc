{
  "ignore": [
    "**/*.min.js"
  ],
  "presets": [
    [
      "env",
      {
        "modules": false,
        "loose": true,
        "useBuiltIns": false,
        "targets": {
          "browsers": [
            "last 4 versions",
            "ie >= 9",
            "iOS >= 7",
            "Android >= 4"
          ]
        }
      }
    ],
    "es2015", "stage-2", "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-runtime",
    // ["import", {"libraryName": "antd-mobile", "style": "true" }]
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": true // `style: true` 会加载 less 文件
    }]
  ]
}
